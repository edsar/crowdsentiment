function first_title(){		
	var rand_number = 0;
	rand_number1 = Math.floor((Math.random()*10));
	rand_number2 = Math.floor((Math.random()*10));
	if( rand_number1 == rand_number2 ){
		rand_number2 = Math.floor((Math.random()*10));
	}
	if( rand_number1 == rand_number2 ){
		if( rand_number2 < 7 )
			rand_number2 = rand_number2 + 2;
		else
			rand_number2 = rand_number2 - 2;
	}
	document.getElementById("artist1").innerHTML = random_artist( rand_number1 );
	document.getElementById("artist2").innerHTML = random_artist( rand_number2 );
	document.getElementById("first_image").innerHTML = "<img src='" + random_image( rand_number1 ) + "' width='165px'/>";
	document.getElementById("second_image").innerHTML = "<img src='" + random_image( rand_number2 ) + "' width='165px'/>";	
}

function random_image( rand_number ) {
	image = new Array(10);
	image[0] = 'http://crescentmoonentertainment.com/bands/245/John-King-Big.jpg';
	image[1] = 'http://www.seethemuzic.com/wp-content/uploads/2013/06/Craig-Morgan-performing-at-The-Downtown-Hoedown-Detroit-MI-June-1st-2013-photo-by-Marc-Nader-1089.jpg';
	image[2] = 'http://americanidol.edogo.com/wp-content/uploads/2010/06/kelliepickler4.jpg';
	image[3] = 'http://4.bp.blogspot.com/-qFeuNdeUaVQ/TazZfg57H1I/AAAAAAAABxA/zPXYZH_mdSM/s640/DSC_4596.jpg';
	image[4] = 'http://nashvillemusicminute.files.wordpress.com/2010/02/nmm_taylor-2.jpg';
	image[5] = 'http://images2.fanpop.com/image/photos/12100000/Rascal-Flatts-ACM-awards-Performance-rascal-flatts-12135608-610-407.jpg';
	image[6] = 'http://www.countrymusicislove.com/wp-content/uploads/2013/07/Danielle-Bradbery-CountryMusicIsLove2.jpg';
	image[7] = 'http://cache4.asset-cache.net/gc/167986853-laura-bell-bundy-performs-during-the-2013-gettyimages.jpg?v=1&c=IWSAsset&k=2&d=GkZZ8bf5zL1ZiijUmxa7QZ%2F0%2FhozZLvojvrgzCSYXU7TuGp%2B4z7i6PGoEvMXUn0ZWMdUGyNRLRj20N7OVYrLIw%3D%3D';
	image[8] = 'http://wac.450f.edgecastcdn.net/80450F/kicks105.com/files/2013/03/cadillac-three.png';
	image[9] = 'http://www.vegasnews.com/wp-content/uploads/6_7_11_C_tim_mcgraw_kabik-62-1-588.jpg';
 	return(image[rand_number]);
 }

 function random_artist( rand_number ) {
	artist = new Array(10);
	artist[0] = 'The John King Band';
	artist[1] = 'Craig Morgan';
	artist[2] = 'Kellie Pickler';
	artist[3] = 'Glen Templeton';
	artist[4] = 'Taylor Swift';
	artist[5] = 'Rascal Flatts';
	artist[6] = 'Danielle Bradbery';
	artist[7] = 'Laura Bell Bundy';
	artist[8] = 'The Cadillac Three';
	artist[9] = 'Tim McGraw';
 	return(artist[rand_number]);
 }
function valButton(btn) {
var cnt = -1;
for (var i=btn.length-1; i > -1; i--) {
   if (btn[i].checked) {cnt = i; i = -1;}
   }
	if (cnt > -1) return btn[cnt].value;
	else return null;
}
function valForm(form) {
	var btn1 = valButton(form.Gender);
	var btn2 = valButton(form.CFun);
	var btn3 = valButton(form.CExciting);
	var btn4 = valButton(form.CAuthentic);
	var btn5 = valButton(form.CThrilling);
	var btn6 = valButton(form.CDaring);
	var btn7 = valButton(form.CSkilled);

	if (btn1 == null)
	{	
		alert('select your Gender');
		return false;
	}else if (btn2 == null)
	{	
		alert('select your Fun');
		return false;
	}else if (btn3 == null)
	{	
		alert('select your Exciting');
		return false;
	}else if (btn4 == null)
	{	
		alert('select your Authentic');
		return false;
	}else if (btn5 == null)
	{	
		alert('select your Thrilling');
		return false;
	}else if (btn6 == null)
	{	
		alert('select your Daring');
		return false;
	}else if (btn7 == null)
	{	
		alert('select your Skilled');
		return false;
	}else
		return true;
}

  function checkvalidation(){
	var objcomment = document.getElementById("Age");
	if(objcomment.value == "")
	{
		alert("Please input your age.");
		return;
	}
    val = valForm(mturk_form);
	if(val == false)
		return;
	var obj = document.getElementById("mturk_form");
	
	
	obj.action = "https://www.mturk.com/mturk/externalSubmit";
	obj.submit();
  }
  turkSetAssignmentID();
