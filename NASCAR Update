<script type="text/javascript" language="javascript">

window.onload=function(){ first_title(); };

function onClicked( id ){
	var artist1_name = document.getElementById("artist1").innerHTML;
	var artist2_name = document.getElementById("artist2").innerHTML;

	if( id == '11' ){
		document.getElementById('Fun1').checked = true;
		document.getElementById('Fun2').checked = true;
		document.getElementById('Fun1').value = "on (" + artist1_name +")";
		document.getElementById('Fun2').value = "off (" + artist2_name +")";
		document.getElementById('fusel1').checked = true;
		document.getElementById('fusel2').checked = false;
	} else if( id == '12' ){
		document.getElementById('Fun1').checked = true;
		document.getElementById('Fun2').checked = true;
		document.getElementById('Fun1').value = "off (" + artist1_name +")";
		document.getElementById('Fun2').value = "on (" + artist2_name +")";
		document.getElementById('fusel2').checked = true;
		document.getElementById('fusel1').checked = false;
	} else if( id == '21' ){
		document.getElementById('Exciting1').checked = true;
		document.getElementById('Exciting2').checked = true;
		document.getElementById('Exciting1').value = "on (" + artist1_name +")";
		document.getElementById('Exciting2').value = "off (" + artist2_name +")";
		document.getElementById('exsel1').checked = true;
		document.getElementById('exsel2').checked = false;
	} else if( id == '22' ){
		document.getElementById('Exciting1').checked = true;
		document.getElementById('Exciting2').checked = true;
		document.getElementById('Exciting1').value = "off (" + artist1_name +")";
		document.getElementById('Exciting2').value = "on (" + artist2_name +")";
		document.getElementById('exsel2').checked = true;
		document.getElementById('exsel1').checked = false;
	} else if( id == '31' ){
		document.getElementById('Authentic1').checked = true;
		document.getElementById('Authentic2').checked = true;
		document.getElementById('Authentic1').value = "on (" + artist1_name +")";
		document.getElementById('Authentic2').value = "off (" + artist2_name +")";
		document.getElementById('ausel1').checked = true;
		document.getElementById('ausel2').checked = false;
	} else if( id == '32' ){
		document.getElementById('Authentic1').checked = true;
		document.getElementById('Authentic2').checked = true;
		document.getElementById('Authentic1').value = "off (" + artist1_name +")";
		document.getElementById('Authentic2').value = "on (" + artist2_name +")";
		document.getElementById('ausel2').checked = true;
		document.getElementById('ausel1').checked = false;
	} else if( id == '41' ){
